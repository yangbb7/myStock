# Staging Environment Configuration for myQuant Frontend

# API Configuration
VITE_API_BASE_URL=https://staging-api.yourdomain.com
VITE_WS_BASE_URL=wss://staging-api.yourdomain.com

# Application Configuration
VITE_APP_TITLE=myQuant 量化交易系统 (Staging)
VITE_APP_VERSION=1.0.0-staging

# Staging Configuration
VITE_DEV_MODE=false
VITE_LOG_LEVEL=debug

# Feature Flags
VITE_ENABLE_MOCK_DATA=false
VITE_ENABLE_DEBUG_TOOLS=true

# Monitoring Configuration
VITE_ENABLE_MONITORING=true

# Error Monitoring
VITE_ERROR_MONITORING_ENABLED=true
VITE_ERROR_MONITORING_ENDPOINT=https://staging-api.yourdomain.com/monitoring/errors
VITE_ERROR_QUEUE_SIZE=50
VITE_ERROR_FLUSH_INTERVAL=15000

# Performance Monitoring
VITE_PERFORMANCE_MONITORING_ENABLED=true
VITE_PERFORMANCE_MONITORING_ENDPOINT=https://staging-api.yourdomain.com/monitoring/performance
VITE_PERFORMANCE_QUEUE_SIZE=25
VITE_PERFORMANCE_FLUSH_INTERVAL=30000

# User Analytics
VITE_USER_ANALYTICS_ENABLED=true
VITE_ANALYTICS_ENDPOINT=https://staging-api.yourdomain.com/monitoring/analytics
VITE_ANALYTICS_QUEUE_SIZE=50
VITE_ANALYTICS_FLUSH_INTERVAL=15000

# Logging Configuration
VITE_CONSOLE_LOGGING=true
VITE_REMOTE_LOGGING=true
VITE_LOGS_ENDPOINT=https://staging-api.yourdomain.com/monitoring/logs
VITE_LOG_QUEUE_SIZE=500
VITE_LOG_FLUSH_INTERVAL=15000

# Third-party Services
VITE_SENTRY_DSN=https://your-staging-sentry-dsn@sentry.io/project-id
VITE_GOOGLE_ANALYTICS_ID=GA-STAGING-XXXXXXXXX
VITE_HOTJAR_ID=

# Security Configuration
VITE_ENABLE_CSP=true
VITE_ENABLE_HSTS=false

# Performance Configuration
VITE_ENABLE_SERVICE_WORKER=false
VITE_ENABLE_PWA=false