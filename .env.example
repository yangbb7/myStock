# 数据源配置
# Tushare Pro Token (免费注册：https://tushare.pro)
TUSHARE_TOKEN=your_tushare_token_here

# 数据源优先级配置
PRIMARY_DATA_PROVIDER=tushare
FALLBACK_DATA_PROVIDERS=yahoo,eastmoney

# Yahoo Finance配置
YAHOO_FINANCE_ENABLED=true

# 东方财富配置
EASTMONEY_ENABLED=true

# API配置
API_HOST=0.0.0.0
API_PORT=8000

# 数据库配置
DATABASE_URL=sqlite:///mystock.db

# Redis配置
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0

# 日志配置
LOG_LEVEL=INFO
LOG_FILE=logs/mystock.log